{
  "name": "react-ssr-egg",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-loader": "^8.0.6",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-react-ssr-egg",
    "stop": "egg-scripts stop --title=egg-server-react-ssr-egg",
    "dev": "nodemon --ext js,jsx --watch components --watch server --watch client  --exec egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "be": "npm-run-all --parallel build_watch:**",
    "d": "nodemon --ext js,jsx --watch components --watch client --watch server --exec 'egg-bin dev' --exec 'webpack --config \"./webpack_server_config\"' --exec 'webpack --config \"./webpack_client_config\"'"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
